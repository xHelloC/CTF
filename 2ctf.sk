function getWgFlag(s: sender, id: string, flag: string, world: string) :: string:
    set {_reg} to {_id} in lowercase
    get flag {_flag} from region {_reg} in world {_world} and save in {_rtn} for {_s}
    {_rtn} is "true":
        return "&aallow"
        stop
    {_rtn} is "false":
        return "&cdeny"
        stop
