function getAmountAllBlocks(s: sender, id: integer, world: object, run: integer):
    if {_run} != 0:
        loop {testblock::items::*}:
            broadcast "&2Item &7[&e %loop-value% &7] : %{testblock::counter::%loop-value%}%"
            if "%loop-value%" is "bedrock block" or "barrier":
                add "%{testblock::counter::%loop-value%}% %loop-value%" to {ctf::instance::%{_id}%::data::blocks[0]::*}
            else:
                add "%{testblock::counter::%loop-value%}% %loop-value%" to {ctf::instance::%{_id}%::data::blocks[1]::*}
            make {_s} execute command "/ctfadmin mapedit detail %{_id}%"


