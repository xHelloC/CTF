function getTypeAllBlocks(s: sender, id: integer, world: object, run: integer = 0):
    {_run} = 0:
        {ctf::instance::%{_id}%::data::blocks[0]::*} or {ctf::instance::%{_id}%::data::blocks[1]::*} or {ctf::instance::%{_id}%::data::blocks[temp]::*} is not set:
            set {blocks::counter::temp} to 0
            delete {-temp::blocks::*}
            set {now} to now
            set {_l1} to location({ctf::instance::%{_id}%::data::locations[1]::x}, {ctf::instance::%{_id}%::data::locations[C]::y}, {ctf::instance::%{_id}%::data::locations[1]::z},{_world})
            set {_l2} to location({ctf::instance::%{_id}%::data::locations[2]::x}, {ctf::instance::%{_id}%::data::locations[2]::y}, {ctf::instance::%{_id}%::data::locations[2]::z}, {_world})
            loop all blocks within block at {_l1} and block at {_l2}:
                set {_} to {ctf::instance::%{_id}%::data::locations[2]::y}
                if round(loop-block's y-coords) is not round({_}):
                    if loop-block's type is not air:
                        add 1 to {blocks::counter::temp}
                        if {-temp::blocks::*} does not contain loop-block's type:
                            #broadcast "&eTRYING TO ADD - &7> %loop-block's type%"
                            add loop-block's type to {-temp::blocks::*}
                else:
                    create bossbar titled "&fProgress of &cgetting all blocks&f! for instance &b%{_id}%" and id "map.timer" for {_s} with progress 0 with colors white
                    timeBlocks( {blocks::counter::temp}, difference between now and {now}, {_id}, {_s} )
                    getAmountAllBlocks2({_s},{_id},{_world})
                    #broadcast "&aFinished Stage1"
                    stop loop
        else:
            message "you alredy have stt a blocks" to {_s}
function getAmountAllBlocks2(s: sender, id: integer, world: object):
    set {-counterBlocks} to 0
    set {_ll1} to location({ctf::instance::%{_id}%::data::locations[1]::x}, {ctf::instance::%{_id}%::data::locations[C]::y}, {ctf::instance::%{_id}%::data::locations[1]::z},{_world})
    set {_ll2} to location({ctf::instance::%{_id}%::data::locations[2]::x}, {ctf::instance::%{_id}%::data::locations[2]::y}, {ctf::instance::%{_id}%::data::locations[2]::z}, {_world})
    loop all blocks within block at {_ll1} and block at {_ll2}:
        set {__} to {ctf::instance::%{_id}%::data::locations[2]::y}
        if round(loop-block's y-coords) is not round({__}):
            if loop-block's type is not air:
                if {-temp::blocks::*} contain loop-block's type:
                    wait 1 tick
                    add 1 to {-counterBlocks}
                    set {testblock::counter::%loop-block's type%} to {testblock::counter::%loop-block's type%}  + 1
                    set {_time} to difference between now and {now}
                    send title "&cAdding a blocks &e[%{-counterBlocks}%]" with subtitle "&e%{_time}% &7%loop-block's type%  %loop-block's x-coords% %loop-block's y-coords% %loop-block's z-coords%" to {_s} with fade in 0 seconds and fade out 0 seconds
                    add "%loop-block's type%" to {ctf::instance::%{_id}%::data::blocks[temp]::*}
        else:            
            broadcast "&aFinished stage2"
            stop loop
    
function timeBlocks(o: integer, time: object, id: integer, s: sender):
    set {_1} to (1 / 100 * {_o})
    set {_100} to (100/100 * {_o})
    loop {blocks::counter::temp} times:
        wait 1 ticks
        add 1 to {_progress}
        remove 1 from {_100}
        edit bossbar "map.timer"  progress to (({_progress}/{_1}))
        #broadcast "&fMax: &c%({_100})%       &a%(({_progress}/{_1}))% %%"
        (({_progress}/{_1})) = 100:
            destroy bossbar "map.timer"
            create bossbar titled "&aSuccess!! &fTime elapsed for instance &b%{_id}%&f is &e%{_time}%" and id "map.timer2" for {_s} with progress 100 with colors green
            wait 1.5 seconds
            destroy bossbar "map.timer2"

